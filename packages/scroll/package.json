{
  "name": "@solid-primitives/scroll",
  "version": "2.0.14",
  "description": "Reactive primitives to react to element/window scrolling.",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/scroll",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "scroll",
    "stage": 2,
    "list": [
      "createScrollPosition",
      "useWindowScrollPosition"
    ],
    "category": "Inputs"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "vite serve dev",
    "page": "vite build dev",
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr",
    "test:watch": "watchlist src test -- npm test"
  },
  "keywords": [
    "monitor",
    "scrollTo",
    "scroll",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/event-listener": "workspace:^2.2.10",
    "@solid-primitives/rootless": "workspace:^1.3.2",
    "@solid-primitives/static-store": "workspace:^0.0.2"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {}
}
